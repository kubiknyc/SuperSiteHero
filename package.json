{
  "name": "jobsight",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "JobSight - Construction Field Management Platform for Superintendents",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:strict": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 1000",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report playwright-report",
    "playwright:install": "playwright install",
    "test:manual": "playwright codegen http://localhost:5174",
    "test:manual:auth": "playwright codegen --save-storage=auth.json http://localhost:5174/login",
    "test:inspect": "playwright test --debug",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:takeoffs": "playwright test takeoffs.spec.ts",
    "test:e2e:takeoffs:ui": "playwright test takeoffs.spec.ts --ui",
    "test:e2e:takeoffs:debug": "playwright test takeoffs.spec.ts --debug",
    "test:e2e:takeoffs:headed": "playwright test takeoffs.spec.ts --headed",
    "test:visual": "playwright test visual-regression.spec.ts",
    "test:visual:update": "playwright test visual-regression.spec.ts --update-snapshots",
    "test:visual:ui": "playwright test visual-regression.spec.ts --ui",
    "test:visual:debug": "playwright test visual-regression.spec.ts --debug",
    "test:visual:headed": "playwright test visual-regression.spec.ts --headed",
    "test:visual:chromium": "playwright test visual-regression.spec.ts --project=chromium",
    "test:visual:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:dark-mode": "playwright test e2e/visual-regression/dark-mode-comprehensive.spec.ts e2e/accessibility/dark-mode-contrast.spec.ts e2e/accessibility/dark-mode-states.spec.ts e2e/theme/theme-functionality.spec.ts",
    "test:dark-mode:full": "playwright test e2e/visual-regression/dark-mode-comprehensive.spec.ts e2e/accessibility/dark-mode-contrast.spec.ts e2e/accessibility/dark-mode-states.spec.ts e2e/theme/theme-functionality.spec.ts --reporter=html",
    "test:contrast": "playwright test e2e/accessibility/dark-mode-contrast.spec.ts",
    "test:states": "playwright test e2e/accessibility/dark-mode-states.spec.ts",
    "test:emails": "node scripts/test-email-notifications.mjs",
    "email:setup": "node scripts/setup-email-config.mjs",
    "test:visual:dark": "playwright test e2e/visual-regression/dark-mode-comprehensive.spec.ts",
    "test:visual:dark:update": "playwright test e2e/visual-regression/dark-mode-comprehensive.spec.ts --update-snapshots",
    "ci:test": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "seed:test": "tsx scripts/seed-test-data.ts",
    "seed:test-users": "tsx scripts/seed-test-users.ts",
    "seed:all": "tsx scripts/seed-all-test-data.ts",
    "seed:data": "tsx scripts/seed-all-authenticated.ts",
    "check:test-user": "tsx scripts/check-test-user.ts",
    "test:e2e:seed": "npm run seed:test && npm run test:e2e",
    "analyze": "vite-bundle-visualizer",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "perf:web-vitals": "tsx tests/performance/web-vitals-baseline.ts",
    "perf:load": "k6 run tests/load/scenarios/auth.js",
    "perf:load:projects": "k6 run tests/load/scenarios/projects.js",
    "perf:load:documents": "k6 run tests/load/scenarios/documents.js",
    "perf:load:stress": "k6 run --env SCENARIO=stress tests/load/scenarios/api-stress.js",
    "perf:load:spike": "k6 run --env SCENARIO=spike tests/load/scenarios/api-stress.js",
    "perf:all": "npm run build && npm run lighthouse && npm run perf:load",
    "pwa:icons": "node scripts/generate-pwa-icons.cjs",
    "ios:icons": "node scripts/generate-ios-icons.cjs",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:sync:android": "npx cap sync android",
    "cap:build": "npm run build && npx cap sync",
    "cap:build:ios": "npm run build && npx cap sync ios",
    "cap:build:android": "npm run build && npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:ios:device": "npx cap run ios --target",
    "cap:run:android": "npx cap run android",
    "cap:live": "npx cap run android --livereload --external",
    "cap:live:ios": "npx cap run ios --livereload --external",
    "cap:doctor": "npx cap doctor",
    "ios:build": "npm run build && npx cap sync ios && npx cap open ios",
    "ios:build:prod": "NODE_ENV=production npm run build && npx cap sync ios",
    "ios:test": "npm run build && npx cap sync ios && npx cap run ios",
    "audit:colors": "tsx scripts/audit-design-system-colors.ts",
    "audit:typography": "tsx scripts/audit-design-system-typography.ts",
    "audit:touch": "tsx scripts/audit-touch-targets.ts",
    "audit:dark": "tsx scripts/audit-dark-mode.ts",
    "audit:all": "npm run audit:colors && npm run audit:typography && npm run audit:touch && npm run audit:dark",
    "audit:dashboard": "npm run audit:all && tsx scripts/generate-audit-dashboard.ts",
    "migrate:colors": "tsx scripts/migrate-colors-to-tokens.ts",
    "migrate:colors:write": "tsx scripts/migrate-colors-to-tokens.ts -- --write",
    "migrate:colors:phase2": "tsx scripts/migrate-colors-phase2.ts",
    "migrate:colors:phase2:write": "tsx scripts/migrate-colors-phase2.ts -- --write",
    "migrate:typography": "tsx scripts/migrate-typography-to-tokens.ts",
    "migrate:typography:write": "tsx scripts/migrate-typography-to-tokens.ts -- --write"
  },
