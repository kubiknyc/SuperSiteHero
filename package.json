    "@tensorflow/tfjs": "^4.22.0",
    "@types/dompurify": "^3.0.5",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.16.1",
    "exceljs": "^4.4.0",
    "exifr": "^7.1.3",
