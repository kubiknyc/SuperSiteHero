{
  "name": "construction-management-platform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Comprehensive construction field management platform for superintendents",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:strict": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report playwright-report",
    "playwright:install": "playwright install",
    "test:manual": "playwright codegen http://localhost:5174",
    "test:manual:auth": "playwright codegen --save-storage=auth.json http://localhost:5174/login",
    "test:inspect": "playwright test --debug",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:takeoffs": "playwright test takeoffs.spec.ts",
    "test:e2e:takeoffs:ui": "playwright test takeoffs.spec.ts --ui",
    "test:e2e:takeoffs:debug": "playwright test takeoffs.spec.ts --debug",
    "test:e2e:takeoffs:headed": "playwright test takeoffs.spec.ts --headed",
    "test:visual": "playwright test visual-regression.spec.ts",
    "test:visual:update": "playwright test visual-regression.spec.ts --update-snapshots",
    "test:all": "npm run test:unit && npm run test:e2e",
    "ci:test": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "seed:test": "tsx scripts/seed-test-data.ts",
    "seed:test-users": "tsx scripts/seed-test-users.ts",
    "seed:all": "tsx scripts/seed-all-test-data.ts",
    "seed:data": "tsx scripts/seed-all-authenticated.ts",
    "check:test-user": "tsx scripts/check-test-user.ts",
    "test:e2e:seed": "npm run seed:test && npm run test:e2e",
    "analyze": "vite-bundle-visualizer",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "perf:web-vitals": "tsx tests/performance/web-vitals-baseline.ts",
    "perf:load": "k6 run tests/load/scenarios/auth.js",
    "perf:load:projects": "k6 run tests/load/scenarios/projects.js",
    "perf:load:documents": "k6 run tests/load/scenarios/documents.js",
    "perf:load:stress": "k6 run --env SCENARIO=stress tests/load/scenarios/api-stress.js",
    "perf:load:spike": "k6 run --env SCENARIO=spike tests/load/scenarios/api-stress.js",
    "perf:all": "npm run build && npm run lighthouse && npm run perf:load",
    "pwa:icons": "node scripts/generate-pwa-icons.cjs",
    "cap:sync": "npx cap sync",
    "cap:build": "npm run build && npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android",
    "cap:live": "npx cap run android --livereload --external"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/camera": "^8.0.0",
