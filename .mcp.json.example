{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--read-only",
        "--project-ref=YOUR_SUPABASE_PROJECT_REF"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      }
    },
    "github-jobsight": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote@latest",
        "https://gitmcp.io/YOUR_GITHUB_USERNAME/JobSight"
      ]
    }
  }
}
